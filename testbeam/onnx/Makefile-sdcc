all: onnxtest onnxtestN
onnxtest: onnxtest.C onnxlib.so onnxlib.h onnxutil.h
	g++ onnxtest.C ./onnxlib.so $(shell root-config --glibs --cflags --libs) ${OFFLINE_MAIN}/lib64/libonnxruntime.so -o onnxtest

onnxtestN: onnxtestN.C onnxlib.so onnxlib.h onnxutil.h
	g++ onnxtestN.C ./onnxlib.so $(shell root-config --glibs --cflags --libs) ${OFFLINE_MAIN}/lib64/libonnxruntime.so -o onnxtestN

onnxlib.so: onnxlib.C onnxlib.h
	g++ onnxlib.C -fPIC -shared ${OFFLINE_MAIN}/lib64/libonnxruntime.so -o onnxlib.so
